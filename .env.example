# Discord機器人令牌 (必需)
# 在 https://discord.com/developers/applications 獲取
DISCORD_TOKEN=your_discord_bot_token_here

# 指令前綴 (可選，預設為 !)
COMMAND_PREFIX=!

# 機器人狀態訊息 (可選)
BOT_STATUS=使用 !help 獲取幫助

# 日誌等級 (可選，預設為 INFO)
# 可選值: DEBUG, INFO, WARNING, ERROR, CRITICAL
LOG_LEVEL=INFO

# 調試模式 (可選，預設為 False)
DEBUG=False
